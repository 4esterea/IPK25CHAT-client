<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <AssemblyName>ipk25chat-client</AssemblyName>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <SatelliteAssembliesForSelfContained>false</SatelliteAssembliesForSelfContained>
    <NeutralResourcesLanguage>en</NeutralResourcesLanguage>
    <PublishSingleFile>True</PublishSingleFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SharpPcap" Version="6.3.0" />
    <EmbeddedResource Remove="Resources\*.resx" />
    <None Remove="cs\**\*;de\**\*;es\**\*;fr\**\*;it\**\*;ja\**\*;ko\**\*;pl\**\*;pt-BR\**\*;ru\**\*;tr\**\*;zh-Hans\**\*;zh-Hant\**\*;" />
  </ItemGroup>
  
</Project>